{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "packageNames": [
        "com.example"
      ],
      "semanticCommitType": "chore",
      "versioning": {
        "pin": {
          "rangeStrategy": "custom",
          "lookupName": "org.springframework.boot",
          "lookupUrl": "https://mvnrepository.com/artifact/org.springframework.boot/spring-boot-starter-web"
        }
      },
      "clientRules": [
        {
          "major": true,
          "minor": true,
          "exclude": ["-alpha"]
        }
      ]
    }
  ]
}
